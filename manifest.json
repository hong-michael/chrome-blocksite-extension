{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Block access to specified websites",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}